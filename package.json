{
  "name": "root",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "yarn app start",
    "web": "yarn app web",
    "deploy": "yarn app deploy",
    "postinstall": "yarn app postinstall",
