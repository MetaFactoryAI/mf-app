{
  "name": "root",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "native": "yarn app start",
    "web": "yarn app next",
    "typecheck": "yarn app typecheck",
    "lint": "eslint \"**/*.{ts,tsx,js}\"",
